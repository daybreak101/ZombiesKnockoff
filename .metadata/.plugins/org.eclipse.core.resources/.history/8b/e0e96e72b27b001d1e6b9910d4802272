package entities.creatures.playerinfo;

import main.Handler;

public class Stats {

	private Handler handler;
	private int kills, downs, score;
	
	public Stats(Handler handler) {
		this.handler = handler;
		setKills(0);
		score = 0;
		downs = 0;
	}

	public int getKills() {
		return kills;
	}

	public void setKills(int kills) {
		this.kills = kills;
	}


}
